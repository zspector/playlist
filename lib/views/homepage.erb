
<div class="row" style="width:100%;">
  <div class="small-6 large-6 small-centered large-centered columns">
    <button id="add-playlist" value="plus-sing">+</button>
    <h4>Create New Playlist</h4>
  </div>
</div>
<div class="row">
  <div class="small-6 large-6 small-centered large-centered columns">
    <ul id="playlists">
      <% if @playlists %>
        <% @playlists.each do |playlist| %>
          <li><%= @playlist.name %> </li>
        <% end %>
      <% end %>
    </ul>
  </div>
</div>
